{
  "expo": {
    "name": "Mizan Money",
    "slug": "mizan-banking-app",
    "version": "1.0.0",
    "scheme": "com.wingi.mizanbankingapp",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/poppins/900Black/Poppins_900Black.ttf"
          ]
        }
      ],
      "expo-secure-store",
      "expo-sqlite",
      "expo-web-browser"
    ],
    "orientation": "portrait",
    "icon": "./assets/mizan-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mizan-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wingi.mizanbankingapp",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mizan-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wingi.mizanbankingapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "zibhzfmeyifbrduiesoh.supabase.co"
            },
            {
              "scheme": "com.wingi.mizanbankingapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "2840f4e8-0d73-4221-a407-eceb34e28db3"
      }
    }
  }
}
